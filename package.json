{
  "name": "sw_app",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "5.x",
    "npm": "3.x"
  },
  "scripts": {
    "postinstall": "npm run build",
    "start": "node server.js",
    "start:dev": "DEBUG=sw_app* node-dev server.js",
    "build": "npm run babel && npm run browserify && npm run sass && gulp build",
    "babel": "babel src/ --out-dir build/ --source-maps inline",
    "sass": "node-sass src/client/css/app.scss --include-path ./node_modules/bootstrap-sass/assets/stylesheets --include-path ./node_modules/font-awesome/scss --include-path ./node_modules/highlight.js/styles --output-style compressed -o public/css/",
    "browserify": "browserify --verbose -t [ babelify ] -g uglifyify src/client/js/index.js -o public/index.js",
    "watchify": "watchify --verbose --debug -t [ babelify ] -p browserify-notify src/client/js/index.js -o public/index.js",
    "watch": "parallelshell 'gulp watch' 'npm run babel -- --watch' 'npm run watchify' 'chokidar \"src/client/css/**/*.scss\" -c \"npm run sass -- --source-map-embed\" --initial'",
    "clean": "gulp clean"
  },
  "devDependencies": {
    "browserify-notify": "^1.1.2",
    "chokidar-cli": "^1.2.0",
    "node-dev": "^3.1.3",
    "parallelshell": "^2.0.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "MD5": "^1.3.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.11.4",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-regenerator-runtime": "^6.5.0",
    "babelify": "^7.3.0",
    "bootstrap": "^3.3.7",
    "bootstrap-sass": "^3.3.7",
    "browserify": "^13.1.0",
    "browserify-shim": "^3.8.12",
    "del": "^2.2.2",
    "ejs": "^2.5.1",
    "express": "^4.14.0",
    "font-awesome": "^4.6.3",
    "glob-promise": "^1.0.6",
    "gulp": "^3.9.1",
    "gulp-environments": "^0.1.1",
    "gulp-iconfont": "^8.0.1",
    "gulp-iconfont-css": "^2.1.0",
    "gulp-replace": "^0.5.4",
    "highlight.js": "^9.6.0",
    "md5-file": "^3.1.1",
    "node-sass": "^3.8.0",
    "uglifyify": "^3.0.2"
  }
}
